                                       <apex:selectOptions value="{!gelst}"/>
                                            <apex:actionSupport status="mySaveStatus1" event="onchange" action="{!actualizar}" reRender="fl01,idusuario,idoferta,idescrituras,idprecio,iddias,idestado,idciudad,idcolonia,idinvasion,idetapa,idrehabilitacion,idrehabilitacion,paginacion,asid"/>
                                    </apex:selectList>
                                </div>
                            </div>
                                   
                        </td>
