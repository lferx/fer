            }
            .botones2:active{
                background-color:#D1DA28 !important;
            }
            .contenedorfiltros{
                width:200px; 
                height:auto;
            }
            .divfiltrolabel{
                cursor:pointer;
                background: url('{!URLFOR($Resource.consolas, 'recursos/icons/flecha-boton.png')}') top right no-repeat;
                background-size: 10% 100%;
                background-color:#cfcfcf;
                color:#0C326B;
                position: reltive;
                width:200px;
                height:18px;
                z-index:1000;
                
                overflow-wrap: break-word;
                overflow: auto;
                border-radius: 40px;
                font-family: 'gotham-rounded-book';
                font-size: 12px;
            }
            .divfiltrolista{
                display:none;
                background-color:red;
                width:auto;
                min-width:200px;
                height:auto;
                position: absolute;
                z-index:1000;
                border:1px solid #000;
                overflow-wrap: break-word;
                overflow: auto;
                border-radius: 7px;
                max-height:500px;
            }
            .labelcontador{
                font-family: "gotham-rounded-book" !important;
                font-size: 20px  !important;
                background: #0C326B;
                border-radius: 60px;
                padding: 10px 10px 10px 10px;
                font-size: 16px;
                color: white;
                margin: 30px 30px !important;
            }






            .pagination {
                display: inline-block;
                background-color: #fff;
            }

            .pagination a {
                font-family: "gotham-rounded-book" !important;
                font-size: 14px  !important;
                color: black;
                float: left;
                padding: 5px 10px;
                text-decoration: none;
                transition: background-color .3s;
            }

            .pagination a.active {
                background-color: #5A2A7E;
                color: white;
            }
            .pagination a:hover:not(.active) {
                background-color: #ddd;
            }

            
            .tdtrue{
                background: linear-gradient(to bottom, rgba(209,218,40,1),rgba(209,218,40,0)) !important;
            }
            .tdfalse{
                background: linear-gradient(to bottom, rgba(201,90,80,1),rgba(201,90,80,0)) !important;                
            }
            .imagestyle{
                width:30px;
                filter: grayscale(30%);
            }
            .styledetalles{
                display: none;
                font-size:12px;
                font-weight: normal;
                max-width: 600px;
                
            }
            .imgth{
                width: 30px;
                height: 30px;
            }
            .sectioncabecera{
                border-radius: 15px 15px 0px 0px;
                background-color: #C95A50;
                height: 40px;
                padding: 7px 2px 0px 2px;
                width: auto;

            }












            .section{
                position: relative;
                border: 1px solid #fff;
                border-spacing: 2px;
                padding-top: 90px;
                background: #fff;
                border-radius:10px;
                width:100%;
                height:100%;
                
            }
            .container {
                display:block;
                min-height: 150px;
                max-height: calc(100vh - 210px);
                height: 100%;
                overflow-y: auto;
                width:100%;
            }
            .table2 {
                border-spacing: 2px;
                width:100%;
            }
            .table2 td,th {
                border-bottom:1px solid #eee;
                background: #fff;
                color: #000;
                padding: 3px 10px;
                max-width:200px;
                width:auto;
                text-align: center;
            }
            .table2 th {
                height: 0;
                line-height: 0;
                padding-top: 0;
                padding-bottom: 0;
                color: transparent;
                border: none;
                white-space: nowrap;
                text-align: center;
            }
