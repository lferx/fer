                    <td rowspan="3"><img src="{!URLFOR($Resource.consolas, 'recursos/imagenes/elemento-der.jpg')}" width="40px" height="20px" /></td>
                </tr>
                <tr style="height: 1px;">
                    <td height="1px" ><h1 style="color: #5A2A7E;">-</h1></td>
