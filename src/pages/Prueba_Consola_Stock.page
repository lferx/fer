                                <div id="fd002" class="divfiltrolista" onmouseover="ftrue('fd002');">
                                    <apex:selectList id="idoferta" styleClass="listadeseleccion" value="{!ofs}">
                                        <apex:selectOptions value="{!oflst}"/>
                                        <apex:actionSupport status="mySaveStatus1" event="onchange" action="{!actualizar}" reRender="fl02,idusuario,idoferta,idescrituras,idprecio,iddias,idestado,idciudad,idcolonia,idinvasion,idetapa,idrehabilitacion,paginacion,asid"/>
                                    </apex:selectList>
                                </div>
                            </div>
                        </td>
                        <td colspan="3"><h2>Escrituras</h2><br/>
                            <div onmouseout="ffalse('fd003')" class="contenedorfiltros">
                                <apex:outputPanel id="fl03">
                                    <div id="fd03" class="divfiltrolabel" onclick="ftrue('fd003');">
                                        {!ess}
                                    </div>
                                </apex:outputPanel>
                                <div id="fd003" class="divfiltrolista" onmouseover="ftrue('fd003');">
                                    <apex:selectList id="idescrituras" styleClass="listadeseleccion" value="{!ess}">
                                        <apex:selectOptions value="{!eslst}"/>
                                        <apex:actionSupport status="mySaveStatus1" event="onchange" action="{!actualizar}" reRender="fl03,idusuario,idoferta,idescrituras,idprecio,iddias,idestado,idciudad,idcolonia,idinvasion,idetapa,idrehabilitacion,paginacion,asid"/>
                                    </apex:selectList> 
                                </div>
                            </div>
                        </td>
