                                        {!IF(prs==5000000,'Todo','Hasta $'+TEXT(prs))}
                                    </div>
                                </apex:outputPanel>
                                <div id="fd004" class="divfiltrolista" onmouseover="ftrue('fd004');">
                                    <apex:selectList styleClass="listadeseleccion" value="{!prs}">
                                        <apex:selectOptions value="{!prlst}"/>
                                        <apex:actionSupport status="mySaveStatus1" event="onchange" action="{!actualizar}" reRender="fl04,idusuario,idoferta,idescrituras,idprecio,iddias,idestado,idciudad,idcolonia,idinvasion,idetapa,idrehabilitacion,paginacion,asid"/>
                                    </apex:selectList>
                                </div>
                            </div>
                        </td>
