        <script type="text/javascript">
            function ftrue(z){
                document.getElementById(z).style.display = 'block';
            }
            function ffalse(y){
                document.getElementById(y).style.display = 'none';
            }
        </script>

        <script>
            $( function() {
                $( "#dialog" ).dialog({
                    autoOpen: false,
                    resizable: false, 
                    width: 700, 
                    height: 600, 
                    autoResize: true, 
                    modal: true, 
                    draggable: true,
                    show: {
                        effect: "fade",
                        duration: 500
                    },
                    hide: {
                        effect:"explode", duration: 600
                    }
        
                });

                $( ".comentariosbotoneditar" ).on( "click", function() {
                    $( "#dialog" ).dialog( "open" );
                });
            });
        </script>
       
        
    </head>


    <body > <apex:form ><apex:actionRegion ><apex:outputPanel >
        <div class="header"><!-- //////////////////    COMIENZA DIV HERDAR   ////////////////////////// -->
