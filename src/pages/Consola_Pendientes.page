              var objeto = document.getElementById(varid);
              var objeto2 = document.getElementById(varid2);
              if(objeto.style.display == 'block'){
                  objeto.style.display = 'none';
                  objeto2.style.display = 'none';
              }else{
                  objeto.style.display = 'block';
              }
              
          }

         
          
