<apex:page standardController="Paquete__c" extensions="PaqueteOfertaController" showHeader="false" >

<head>
    <apex:stylesheet value="{!URLFOR($Resource.jquery_ui,'css/smoothness/jquery-ui-1.8.23.custom.css')}"/>
    <apex:includeScript value="{!URLFOR($Resource.jquery_ui,'js/jquery-1.8.0.min.js')}"/>
    <apex:includeScript value="{!URLFOR($Resource.jquery_ui,'js/jquery-ui-1.8.23.custom.min.js')}"/>
    <apex:includeScript value="{!URLFOR($Resource.jquery_ui,'postmessage/jquery.ba-postmessage.js')}"/>
    
    <style class="text/css">
    #queryOfertasLoading {
        display: none;
        vertical-align: middle;
    }
    .addOfertaHeader, .delOfertaHeader {
        width: 10px;
    }
    .addOferta, .delOferta {
        background: url(data:image/png;base64,) no-repeat;
        width: 14px;
        height: 14px;
        margin-left: -2px;
    }
    .addOferta { background-position: 0% 0%; }
    .delOferta { background-position: -14px 0%; }
    .addOferta:hover { background-position: -28px 0%; }
    .delOferta:hover { background-position: -42px 0%; }
    #pageTools {
        position: relative;
    }
    #pageTools #pageSizeArrow {
        background: url(/img/func_icons/util/selectArrow12.gif) 0% 0%;
        height: 12px;
        width: 14px;
        margin: 1px 20px 0 3px;
        display: block;
        float: left;
    }
    #pageTools #pageSizeSelector {
        background-color: #DDD;
        border: #CCC solid 2px;
        border-collapse: separate;
        color: #222;
        cursor: pointer;
        display: none;
        height: 75px;
        width: 214px;
        margin: 0;
        position: relative;
        margin-top:-79px;
    }
    #pageTools #pageSizeSelector div {
        font-family: Arial, Helvetica, sans-serif;
        font-size: 12px;
        width: 100%;
        height: 15px;
        text-align: center;
        display: block;
    }
    #pageTools #pageSizeSelector div:hover {
        background-color: #E1F6FF;
        color: #000;
    }
    #pageTools #pageSizeSelector div div {
        background-color: #FFF;
        font-weight: bold;
        height: 100%;
        width: 30px;
        display: inline-block;
        color: #000;
    }
    #pageTools .pageGrid {
        background: url(/img/alohaSkin/listFooter.png) repeat-x;
    }
    #pageTools .pageGridCols {
        width: 33.3333%;
    }
    #pageTools td {
        vertical-align: middle !important;
    }
    #pageTools #pageSizeBlock {
        height: 18px;
    }
    #pageTools #pageStatus {
        background: url(/img/loading.gif) no-repeat;
        height: 16px;
        float: right;
        display: none;
        font-weight: bold;
        padding-left: 20px;
        padding-top: 2px;
    }
    #pageTools .linkGrid {
        margin: auto;
    }
    #pageTools .pageFirst, #pageTools .pagePrevious, #pageTools .pageNext, #pageTools .pageLast {
        background: url(/img/paginationArrows.gif) no-repeat;
        width: 9px;
        height: 10px;
        margin: 1px 4px;
        display: block;
    }
    #pageTools .pageFirst {
        background-position: 0px 1px;
    }
    #pageTools .pagePrevious {
        background-position: -10px 1px;
        float: left;
    }
    #pageTools .pageNext {
        background-position: -17px 1px;
        float: right;
    }
    #pageTools .pageLast {
        background-position: -27px 1px;
    }
    #pageTools .pageLink {
        text-decoration: none;
        margin-left: 4px;
        margin-right: 4px;
    }
    #pageTools #pageSelector {
        float: right;
    }
    #pageTools #pageNumber {
        text-align: center;
        width: 23px;
        margin: 0px;
        padding: 0px;
    }
    </style>
    <script type="text/javascript">
        function cancel() {
            return confirm("¿Está seguro que desea cancelar?");
        }
        
        function toggle(id) {
            $("#"+id).toggle();
        }
        
        function autoComplete(field) {
            $(field).autocomplete({
                source: function(request, response) {
                    PaqueteOfertaController.search($(field).val(), function(result, event) {
                        if (event.status) {
                            response(result);
                        } else {
                            console.log(event);
                        }
                    }, {escape:false});
                },
                minLength: 3,
                select: function(event, ui) {
                    event.preventDefault();
                    this.value = ui.item.value;
                    search();
                },
                focus: function(event, ui) {
                    event.preventDefault();
                    this.value = ui.item.value;
                }
            });
        }
        
        function search() {
            queryOfertas($("#searchText").val());
        }
        
        function showSizes() {
            toggle('pageSizeSelector');
            if($("#pageSizeSelector").css("display") == 'none') {
                pageSizeArrow.style.backgroundPosition = '0% 0%';
            } else {
                pageSizeArrow.style.backgroundPosition = '-15px 0';
            }
        }
        
        var currentOrderBy;
        var sortOrder = 'ASC';
        function setOrderBy(field) {
            if(currentOrderBy == field) {
                sortOrder = sortOrder == 'ASC' ? 'DESC' : 'ASC';
            } else {
                sortOrder = 'ASC';
            }
            currentOrderBy = field;
            orderBy(currentOrderBy, sortOrder);
        }
        
        function setRowColor() {
            $("input[type='hidden'][name='paquete']").each(function() {
                if($(this).val() == "") {
                    $(this).parent().parent().css("background","#CFC");
                }
            });
        }
        
         
        
        function closeModalDialog() {
            var j$ = jQuery.noConflict();
            var parent_domain = '{!$CurrentPage.parameters.parent_domain}';
            var cross_result = new Object();
            cross_result.action = 'close_modal_dialog';
        
            j$.postMessage(
                cross_result,
                parent_domain,
                parent
            );
        }
        
        $(document).ready(function() {
            $("#searchText").keypress(function(event) {
                if(event.keyCode == 13) {
                    search();
                    return !(window.event && window.event.keyCode == 13);
                }
            });
        });
        </script>
</head>
<apex:form >
    <apex:actionStatus id="queryOfertasStatus" onstart="toggle('queryOfertasLoading')" onstop="toggle('queryOfertasLoading')"/>
    <apex:actionStatus id="pageStatus" onstart="toggle('pageStatus')"/>
    <apex:actionFunction action="{!queryOfertas}" name="queryOfertas" status="queryOfertasStatus" rerender="searchResults">
        <apex:param assignTo="{!ofertaName}" name="OfertaName" value=""/>
    </apex:actionFunction>
    <apex:actionFunction action="{!queryOfertas}" name="orderBy" status="pageStatus" rerender="searchResults">
        <apex:param assignTo="{!orderBy}" name="orderBy" value=""/>
        <apex:param assignTo="{!sortOrder}" name="sortOrder" value=""/>
    </apex:actionFunction>
    <apex:actionFunction action="{!addOferta}" name="addOferta" status="queryOfertasStatus" rerender="searchResults, pbOfertas">
        <apex:param name="addOfertaId" value=""/>
    </apex:actionFunction>
    <apex:actionFunction action="{!delOferta}" name="delOferta" status="queryOfertasStatus" rerender="searchResults, pbOfertas">
         <apex:param name="delOfertaId" value=""/>
    </apex:actionFunction>
    <apex:pageBlock id="pbBusquedaOfertas">
         <apex:tabPanel switchType="client">
            <apex:tab label="Búsqueda de ofertas">
            <div>
              <span>Por palabra clave:</span><br/>
              <input type="text" size="20" title="Buscar" onkeyup="return autoComplete(this)" id="searchText"/>
              <input type="button" value="Buscar" onclick="search()"/>
              <img id="queryOfertasLoading" src="data:image/gif;base64,"/>
            </div>
            <br/>
            <apex:outputPanel id="searchResults">
                <apex:pageMessages />
                <apex:outputText value="No hay resultados que mostrar" rendered="{!ofertas.size == 0}"/>
                <apex:outputPanel rendered="{!ofertas.size > 0}">
                        <apex:pageBlockTable value="{!ofertas}" var="item">
                         <apex:column styleClass="addOfertaHeader">
                                <div class="addOferta" onclick="addOferta('{!item.Id}')"></div>
                        </apex:column>
                        <apex:column >
                            <apex:facet name="header">
                              <div onclick="setOrderBy('Name')">Oferta</div>
                            </apex:facet>
                            <apex:outputField value="{!item.Name}"/>
                       </apex:column>
                       <apex:column >
                            <apex:facet name="header">
                              <div onclick="setOrderBy('Direccion__c')">Dirección</div>
                            </apex:facet>
                            <apex:outputField value="{!item.Direccion__c}"/>
                       </apex:column>
                        <apex:column >
                            <apex:facet name="header">
                              <div onclick="setOrderBy('Colonia__c')">Colonia</div>
                            </apex:facet>
                            <apex:outputField value="{!item.Colonia__c}"/>
                       </apex:column>
                    </apex:pageBlockTable>
                    <div id="pageTools">
                        <apex:actionFunction action="{!queryOfertas}" status="pageStatus" rerender="searchResults" name="setPageSize">
                            <apex:param assignTo="{!pageSize}" name="pageSize" value=""/>
                         </apex:actionFunction>
                         <apex:actionFunction action="{!queryOfertas}" status="pageStatus" rerender="searchResults" name="setPageNumber">
                            <apex:param assignTo="{!pageNumber}" name="pageNumber" value=""/>
                         </apex:actionFunction>
                         <apex:actionStatus id="pageStatus" onstart="toggle('pageStatus')"/>
                         <div id="pageSizeSelector">
                            <div onclick="setPageSize('10')">Visualización<div>10</div>registros por página</div>
                            <div onclick="setPageSize('25')">Visualización<div>25</div>registros por página</div>
                            <div onclick="setPageSize('50')">Visualización<div>50</div>registros por página</div>
                            <div onclick="setPageSize('100')">Visualización<div>100</div>registros por página</div>
                            <div onclick="setPageSize('200')">Visualización<div>200</div>registros por página</div>
                        </div>
                         <apex:panelGrid columns="3" width="100%" styleClass="pageGrid" columnClasses="pageGridCols">
                            <div id="pageSizeBlock">
                              <span>{!pageStart} - {!pageEnd} de {!resultSize}</span>
                              <div id="pageSizeArrow" onclick="showSizes()"></div>
                              <div id="pageStatus">Cargando...</div>
                            </div>
                            <apex:panelGrid columns="4" styleClass="linkGrid">
                              <apex:commandLink status="pageStatus" rerender="searchResults" action="{!first}" rendered="{!pageNumber > 1}"><span class="pageFirst"></span></apex:commandlink>
                              <apex:commandLink status="pageStatus" rerender="searchResults" action="{!previous}" rendered="{!pageNumber > 1}" styleClass="pageLink"><span class="pagePrevious"></span>Anterior</apex:commandlink>
                              <apex:commandLink status="pageStatus" rerender="searchResults" action="{!next}" rendered="{!pageNumber < totalPages}" styleClass="pageLink">Siguiente<span class="pageNext"></span></apex:commandlink>
                              <apex:commandLink status="pageStatus" rerender="searchResults" action="{!last}" rendered="{!pageNumber < totalPages}"><span class="pageLast"></span></apex:commandlink>
                            </apex:panelGrid>
                            <div id="pageSelector">
                              <span>Página </span><input type="text" value="{!pageNumber}" onkeydown="setPageNumber(this.value)" onchange="setPageNumber(this.value)" id="pageNumber"/><span> de {!totalPages}</span>
                            </div>
                      </apex:panelGrid>
                    </div>
                </apex:outputPanel>
            </apex:outputPanel>
            </apex:tab>
         </apex:tabPanel>
    </apex:pageBlock>
    <apex:pageBlock id="pbOfertas" title="Ofertas del paquete">
    <apex:outputText value="No hay Ofertas" rendered="{!OfertasAdded.size == 0}"/>
    <apex:pageBlockTable value="{!OfertasAdded}" var="item" rendered="{!OfertasAdded.size > 0}">
      <apex:column styleClass="delOfertaHeader">
        <div class="delOferta" onclick="delOferta('{!item.Id}')"></div>
        <input type="hidden" value="{!item.Paquete__c}" name="paquete"/>
      </apex:column>
      <apex:column headerValue="Nombre oferta"><apex:outputField value="{!item.Name}"/></apex:column>
      <apex:column headerValue="Etapa oferta"><apex:outputField value="{!item.Etapa__c}"/></apex:column>
      <apex:column headerValue="Calle"><apex:outputField value="{!item.Direccion__c}"/></apex:column>
      <apex:column headerValue="Colonia"><apex:outputField value="{!item.Colonia__c}"/></apex:column>
    </apex:pageBlockTable>
    <script type="text/javascript">setRowColor();</script>
    <apex:pageBlockButtons location="bottom">
      <apex:commandButton action="{!save}" value="Guardar" rerender="searchResults, pbOfertas" oncomplete="closeModalDialog();"/>
      <apex:commandButton action="{!cancel}" value="Cancelar" onclick="closeModalDialog();"/>
    </apex:pageBlockButtons>
    </apex:pageBlock>
</apex:form>

</apex:page>